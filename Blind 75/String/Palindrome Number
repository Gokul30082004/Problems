class Solution {
    public boolean isPalindrome(int x) {
        if(x<0) return false;
        String s=String.valueOf(x);
        int n=s.length()-1;
        for(int i=0;i<=n;i++)
        {
            if(s.charAt(i)!=s.charAt(n)){
                return false;
            }
            --n;
        }
        return true;
    }
}
