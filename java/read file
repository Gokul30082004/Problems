import java.io.*;
import java.util.*;
public class file{
    public static void main(String[] args) throws Exception{
        File myfile=new File("filetxt.txt");
        Scanner inp=new Scanner(myfile);
        while(inp.hasNext())
            // inp.next() for going to next
            // inp.nextInt() get as input
            System.out.println(inp.nextLine());
        inp.close();
    }
}
